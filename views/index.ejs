<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta http-equiv="X-UA-Compatible" content="IE=edge" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0" />
		<title>Store Index</title>
	</head>
	<h1>Store</h1>
	<body>
		<a href="/store/new/">New Product</a>
		<ul>
			<% products.forEach(product => { %>
			<li><%=product.name%></li>
				<li><a href="/store/<%=product._id%>"><img src="<%=product.img%>"/></a>
			</li>
			<% }) %>
		</ul>
	</body>
</html>
